{
  "name": "antd-mobile-taro-ui",
  "version": "0.0.2",
  "description": "以antd-mobile为设计标准，基于taro框架的微信小程序组件库",
  "author": "7",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "**/*.scss"
  ],
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xz-77/antd-mobile-taro-ui.git"
  },
  "bugs": {
    "url": "https://github.com/xz-77/antd-mobile-taro-ui/issues"
  },
  "homepage": "https://github.com/xz-77/antd-mobile-taro-ui#readme",
  "keywords": [
    "antd-mobile",
    "Taro",
    "miniprogram",
    "wechat"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/es/index.js",
  "types": "./lib/es/index.d.ts",
  "typings": "./lib/es/index.d.ts",
  "scripts": {
    "build:alipay": "taro build --type alipay",
    "build:h5": "taro build --type h5 && yarn run deploy",
    "build:jd": "taro build --type jd",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "build:rn": "taro build --type rn",
    "build:swan": "taro build --type swan",
    "build:tt": "taro build --type tt",
    "build:weapp": "taro build --type weapp",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:h5": "taro build --type h5 --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:weapp": "npm run build:weapp -- --watch",
    "build": "gulp",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "release": "standard-version",
    "deploy": "gh-pages -d dist/h5",
    "install-peers": "install-peerdeps"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.4.11",
    "@tarojs/plugin-framework-react": "3.4.11",
    "@tarojs/react": "3.4.11",
    "@tarojs/runtime": "3.4.11",
    "@tarojs/taro": "3.4.11",
    "ahooks": "^3.6.2",
    "antd-mobile": "^5.19.0",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "lorem-ipsum": "^2.0.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-native": "^0.68.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@tarojs/mini-runner": "3.4.11",
    "@tarojs/webpack-runner": "3.4.11",
    "@types/classnames": "^2.3.1",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-preset-taro": "3.4.11",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "del": "^6.1.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-taro": "3.4.11",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^43.0.0",
    "gh-pages": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-less": "^5.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^7.0.4",
    "install-peerdeps": "^3.0.3",
    "lint-staged": "^12.3.7",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "standard-version": "^9.5.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "typescript": "~4.6.4",
    "verify-fabric": "1.5.0"
  },
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
